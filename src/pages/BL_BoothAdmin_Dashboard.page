pageSize: 10,
proxy: {
 type: 'pagingmemory',
 data: myData1,
 reader: {
  type: 'array'
 }
}
});
     
 
// geography chart store 

var store = Ext.create('Ext.data.JsonStore', {  
    fields: ['namegeo', 'datageo'], 
