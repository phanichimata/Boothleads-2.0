store: geogridstore,
displayInfo: true
}),
listeners:
{
afterrender: function (component)
{
component.down('#refresh').hide();
}
},
viewConfig:{
 loadMask:false
},
columns: [
{header:'id', dataIndex: 'id',hidden:true }, 
{
header: 'Name',  dataIndex: 'Name',
renderer: function(value, p, r)           
{
 return r.data['FirstName']+ ' ' + r.data['LastName'];
}
},   
