 }
},   
{
 header:"<b> Program Name </b>",dataIndex:'prname',flex:1.5,
 renderer: function(value, p, r){
  return '<a href="#" onClick="selectOneCheckBox(\''+r.data['id']+'\')" style=\"color:blue;text-decoration:underline;\"> '+ r.data['prname']+'</a>';}
},
        {
         header:"<b> Program Date </b>",flex:1.25,dataIndex:'Programdate'}
        ,
        {
         header:"<b> Start Time </b>",flex:1,dataIndex:'Starttime'}
        ,
        {
         header:"<b> End Time </b>",flex:1,dataIndex:'Endtime'}       
        ,
        {
         header:"<b> Presenter </b>",flex:1,dataIndex:'Presenter'}        
         ,
        {
         header:"<b> Location </b>",flex:1,dataIndex:'Location'}
      ],
height: 350, 
width: '100%',
title: GridTitle,
stripeRows:true,
bbar: new Ext.PagingToolbar({
        store: store,
        pageSize: 50,
        displayInfo: true  
}),
renderTo: 'items-grid1'                            
});

  
store.loadPage(1);

});
