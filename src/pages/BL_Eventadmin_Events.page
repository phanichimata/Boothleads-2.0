fields:[{name:'id'},{name:'prname'},{name:'Programdate'},{name:'Starttime'},{name:'Endtime'},{name:'Presenter'},{name:'Location'}],
pageSize: 50,
proxy: {
type: 'direct',
directFn: myDirectfn,
reader: {
type: 'array'
}
},
});
store.loadData(myData1);
      
 


grid = Ext.create('Ext.grid.Panel',{
store: store, 
viewConfig: {
id: 'gv',
trackOver: true,
stripeRows: true,
loadMask: false
},
listeners:
{
afterrender: function (component)
{
component.down('#refresh').hide();
}
},         
columns: [
{header:'id', dataIndex: 'id',hidden:true },   
{
