              return false;
      }
      if( lvAgId.value == ""){
              alert("Please Furnish Event End Date");
              lvAgId.focus();
              return false;
      }else{
          if(lvAgId.value < lvMnId.value){
             
              alert("Event End Date should not be less than Start Date");
              lvAgId.focus();
              return false;
          } 
      }         
      if(lvEtId.value == ""){
              alert("Event End Time should not be None");
              lvEtId.focus();
              return false;
      }        
      
      var startdate = Date.parse(lvMnId.value);
      var enddate = Date.parse(lvAgId.value); 
         
        if(startdate>enddate){
            alert("Event End Date must be greater than Event Start Date");
            lvAgId.focus();
            return false;
        }
       if(zip.value != "")
