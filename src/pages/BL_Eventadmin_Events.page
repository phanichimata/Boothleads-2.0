        var todayDate = '<apex:outputText value="{0,date,MM/dd/yyyy}"><apex:param value="{!todaysDate}" /></apex:outputText>';
        
        
         
          
         
        
         
         if(lvTnId.value == ""){
              alert("Please Furnish Event Title");
              lvTnId.focus();
              return false;
                 }
         
         if(lvPnId.value == ""){
              alert("Event Hosting TimeZone should not be None");
              lvPnId.focus();
              return false;
      }
            
      if(lvMnId.value == ""){
              alert("Please Furnish Event Start Date");
              lvMnId.focus();
              return false;
      }else{
          if(lvMnId.value <= todayDate ){
             
              alert("Event Start Date should not be greater than Today's Date");
              lvMnId.focus(); 
              return false;
          }
      }
                       
      if(lvStId.value == ""){
              alert("Event Start Time should not be None");
